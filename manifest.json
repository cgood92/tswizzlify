{
   "background": {
      "scripts": [ "tswizzlify/bg.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "tswizzlify/tswizzlify.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "tswizzlify/logo128.png",
      "16": "tswizzlify/logo16.png"
   },
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "options_page": "tswizzlify/options/options.html",
   "options_ui": {
      "chrome_style": true,
      "page": "tswizzlify/options/options.html"
   },
   "permissions": [ "storage", "tabs", "contextMenus", "webRequest", "\u003Call_urls>", "http://*/*", "https://*/*", "webNavigation", "webRequestBlocking", "cookies", "storage", "unlimitedStorage" ],
   "short_name": "__MSG_appName__",
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
